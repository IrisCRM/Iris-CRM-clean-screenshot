{
  "name": "Iris CRM - Clean screenshot",
  "version": "1.1",
  "description": "Плагин для упрощения создания скриншотов карточек в Iris CRM.",
  "permissions": ["tabs", "http://*/*"],
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["j"]
    }
  ],
  "browser_action": {
    "default_title": "Для скриншов в Iris CRM",
    "default_icon": "iris.png",
    "popup": "popup.html"
  }
}
